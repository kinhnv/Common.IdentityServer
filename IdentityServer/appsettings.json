{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Mssql": {
    "ConnectionString": "Data Source=${COMMON_SQL_HOST},${COMMON_SQL_PORT};database=${COMMON_IDENTITY_SERVER_SQL_DB};Persist Security Info=True;User ID=${COMMON_SQL_USER};Password=${COMMON_SQL_PASSWORD}"
  },
  "Elasticsearch": {
    "Url": "http://${COMMON_ES_USER:elastic}:${COMMON_ES_PASSWORD}@${COMMON_ES_HOST}:${COMMON_ES_PORT:9200}"
  },
  "InfrastructurePath": "../Infrastructure/bin/Debug/net6.0/Infrastructure.dll"
}
